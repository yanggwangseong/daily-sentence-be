{
    "name": "ts-node-starter",
    "version": "1.0.0",
    "description": "ts-node-starter",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "nest build",
        "test": "jest",
        "start:dev": "nest start --watch",
        "lint": "eslint '*/**/*.ts'",
        "eslint:fix": "eslint src --fix",
        "prettier:fix": "prettier . -w",
        "test:cov": "jest --coverage",
        "test:unit": "jest --config jest.config.js",
        "test:e2e": "jest --config test/jest-e2e.json",
        "test:db:start": "docker compose --env-file .test.env -f docker-compose.test.yml up -d",
        "test:db:stop": "docker compose -f docker-compose.test.yml down -v",
        "test:db:e2e": "npm run test:db:start && sleep 4 && npm run test:e2e -- --runInBand --verbose && npm run test:db:stop",
        "prepare": "husky"
    },
    "author": {
        "name": "yanggwangseong",
        "email": "soaw83@gmail.com",
        "url": "https://github.com/yanggwangseong"
    },
    "license": "MIT",
    "dependencies": {
        "@nestjs/common": "^11.0.17",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.0.17",
        "@nestjs/platform-express": "^11.0.17",
        "@nestjs/typeorm": "^11.0.0",
        "discord.js": "^14.18.0",
        "dotenv": "^16.5.0",
        "mysql2": "^3.11.4",
        "necord": "^6.8.8",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typeorm": "^0.3.22"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.6",
        "@nestjs/schematics": "^11.0.5",
        "@nestjs/testing": "^11.0.17",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/express": "^5.0.1",
        "@types/jest": "^29.5.14",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.26.0",
        "@typescript-eslint/parser": "^8.26.0",
        "eslint": "^9.21.0",
        "eslint-config-prettier": "^10.0.2",
        "eslint-plugin-prettier": "^5.2.3",
        "husky": "^9.1.7",
        "jest": "^29.7.0",
        "prettier": "^3.5.3",
        "rimraf": "^6.0.1",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.2",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "5.6.3"
    }
}
